{
  "name": "client-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "gen": "graphql-codegen --config codegen.yml && node src/post-processing",
    "dev:ssr": "ng run client-angular:serve-ssr",
    "serve:ssr": "node dist/client-angular/server/main.js",
    "build:ssr": "ng build && ng run client-angular:server",
    "prerender": "ng run client-angular:prerender",
    "lint": "ng lint",
    "lint-css": "stylelint src/**/*.scss",
    "prettier": "prettier --write src/**/*.{js,ts,scss}"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint"
    }
  },
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/platform-server": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@apollo/client": "^3.6.9",
    "@graphql-codegen/cli": "^2.7.0",
    "@graphql-codegen/typescript-graphql-request": "^4.4.10",
    "@ngrx/effects": "^14.0.1",
    "@ngrx/store": "^14.0.1",
    "@ngrx/store-devtools": "^14.0.1",
    "@nguniversal/express-engine": "^14.0.3",
    "apollo-angular": "^3.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.15.2",
    "graphql": "^16.5.0",
    "ngx-cookie-service": "^14.0.1",
    "prettier-eslint": "^15.0.1",
    "reset-css": "^5.0.1",
    "rxjs": "~7.5.0",
    "save-dev": "^0.0.1-security",
    "sprintf-js": "^1.1.2",
    "swiper": "^8.3.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.4",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/schematics": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "~14.0.4",
    "@angular/compiler-cli": "^14.0.0",
    "@graphql-codegen/cli": "2.7.0",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.6.0",
    "@graphql-codegen/typescript-apollo-angular": "3.4.13",
    "@graphql-codegen/typescript-operations": "^2.4.3",
    "@nguniversal/builders": "^14.0.3",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^14.15.0",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "eslint": "^8.18.0",
    "husky": "^4.3.8",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "pretty-quick": "^3.1.3",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "typescript": "~4.7.2"
  }

# GraphQL schema example
#
# https://gqlgen.com/getting-started/


type Query {
  # Получить инфу
  getInfo: Info!

  # Получить работы
  getWorks: [Work]!
}

type Mutation {
  # Обновить инфу
  updateInfo(input: updateInfo!): Info!

  # Обновить работу
  updateWork(input: updateWork!): Work!

  # Удалить работу
  deleteWork(input: deleteWork!): Work!
}


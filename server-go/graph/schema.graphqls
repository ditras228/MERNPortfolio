type Query {
  # Получить инфу
  getInfo: GetInfo!

  # Получить работы
  getWorks: [GetWork]!
}

type Mutation {
  # Обновить инфу
  updateInfo(input: UpdateInfo!): GetInfo!

  # Обновить работу
  updateWork(input: UpdateWork!): GetWork!

  # Удалить работу
  deleteWork(input: DeleteWork!): GetWork!

  # Логин
  auth(input: UserInput!): UserOutput!
}


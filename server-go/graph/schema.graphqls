type Query {
  # Получить инфу
  getInfo: GetInfo!

  # Получить работы
  getWorks: [GetWork]!

  # Получить теги
  getTags: [GetTag]!
}

type Mutation {
  # Обновить инфу
  updateInfo(input: UpdateInfoInput!): GetInfo!

  # Обновить работу
  updateWork(input: UpdateWorkInput!): GetWork!

  # Удалить работу
  deleteWork(input: DeleteWorkInput!): GetWork!

  # Логин
  auth(input: UserInput!): UserOutput!
}

